<h2>Search Authors</h2>
<form action="/authors" method="GET">
  <label for="name">Name</label>
  <input type="text" id="name" name="name" value="<%= searchOptions.name %>">
  <button type="submit">Search</button>
</form>
<div>
  <% authors.forEach((author) => { %>
    <div><%= author.name %></div>
    <a href="#">View</a>
    <a href="#">Edit</a>
  <% }); %>
</div>
